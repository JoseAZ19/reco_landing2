{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "sveltekit",
  "outputDirectory": ".svelte-kit/output",
  "trailingSlash": false,
  "cleanUrls": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/restaurantes", "dest": "/restaurantes" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
} 